      <div id="page-wrapper">
      		<div class="page-header" style="margin-top:0;">		
      			<div class="row">			
      	 			<h1 class="col-xs-8 col-sm-4 text-center text-left-sm"><i class="fa fa-dashboard page-header-icon"></i>&nbsp;&nbsp;<strong>GEO FENCING</strong></h1>
              
              <!--<div class="col-xs-4 col-md-offset-4 alert alert-warning alert-dismissable">
                <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                <strong>Warning!</strong> Better check yourself, you're not looking too good.
              </div>-->
      			
            </div>
      		</div>

          <div class="row">
      		   <div class="col-lg-8" style="padding-right:40px;">
              <div class="row panel panel-info">
                <div class="panel-heading">
                  <h3 class="panel-title"><i class="fa fa-long-arrow-right"></i>Geo fencing map</h3>
                </div>
                <div id="map" class="pannel-body" style="height:800px;">
                </div>
              </div>
             </div>

      		   <div class="col-lg-4">        			
              <div class="row panel panel-info">
                  <div class="panel-heading">
                    <h3 class="panel-title"><i class="fa fa-long-arrow-right"></i>Configuratoins</h3>
                  </div>
                  <div class="panel-body">
                    <form class="form-horizontal" role="form">
                      <div class="form-group">
                        <label for="cepsocket" class="col-sm-2 control-label">CEPURL</label>
                        <div class="col-sm-5">
                          <input type="email" class="form-control" id="cepsocket" placeholder="CEP URL">
                        </div>
                      </div>
                      <div class="form-group">
                        <label for="cepusername" class="col-sm-2 control-label">username</label>
                        <div class="col-sm-5">
                          <input type="email" class="form-control" id="cepusername" placeholder="User Name">
                        </div>
                      </div>
                      <div class="form-group">
                        <label for="ceppassowrd" class="col-sm-2 control-label">password</label>
                        <div class="col-sm-5">
                          <input type="password" class="form-control" id="ceppassword" placeholder="Password">
                        </div>
                      </div>
                   </form>
                  </div>                      
              </div>
              <div class="row panel panel-info">
                  <div class="panel-heading">
                    <h3 class="panel-title"><i class="fa fa-long-arrow-right"></i> Process</h3>
                  </div>
                  <div class="panel-body">
                    <form class="form-horizontal" role="form">
                      <div class="form-group">
                        <label for="inputEmail3" class="col-sm-3 control-label">Execution Plan</label>
                        <div class="col-sm-9">
                          <div class="btn-group">
                            <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown">
                              Execution plans
                              <span class="caret"></span> 
                              <span class="input-group-btn">
                                <button class="btn btn-default" type="button" id="getexeplans" onClick="gisClient.fetchAvailableExecutionList();"><i class="fa fa-refresh"></i></button>
                              </span>
                            </button>
                            <ul class="dropdown-menu" id="executoinList">
                             
                            </ul>
                          </div>
                        </div>
                      </div>
                      <div class="form-group">                        
                        <div class="col-sm-12">
                          <textarea class="form-control" rows="3" placeholder="select an execution plan" id="exeplantxtarea"></textarea>
                        </div>
                      </div>
                      <label class="col-sm-3">Polygon</label>      
                      <div class="form-group">                                         
                        <div class="col-sm-12">
                          <textarea class="form-control" rows="3" placeholder="Darw a polygon on the map to generate the geojson" id="polygon"></textarea>
                        </div>
                      </div>
                      <div class="form-group">
                        <div class="col-sm-9 col-md-offset-3">
                          <p>
                            <button type="button" class="btn btn-primary btn-xs" id="updatelisten" onClick="gisClient.updateExeAndConnectToCEP();">Update CEP and listen</button>
                            <button type="button" class="btn btn-default btn-xs" id="disconnectcep" onClick="gisClient.disconnectFromCEP();">Disconnect from CEP</button>

                          </p>
                        </div>
                      </div>
                   </form>                    
                  </div>                      
              </div>

              <div class="row panel panel-info">
                  <div class="panel-body ">
                    <p>
                    <span class="avatar">
                      <button class="btn btn-default" type="button" onClick="gisClient.clearConsole();"><i class="fa fa-refresh"></i></button>
                    </span>Console <label class="col-sm-5" style="float:right;">Websocket status :<span class="avatar"> <img id="socketindicator" src="assets/icons/red_indicator.png" style="width: 20px;float:right;"></span></label></p>                 
                    <textarea class="form-control" rows="3" id="console" rows="5" cols="60"></textarea>  
                  </div>                      
              </div>
      	   </div>	
  	
